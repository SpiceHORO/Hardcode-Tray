{
    "name": "ODrive",
    "app_path": [
        "/opt/OpenDrive/",
        "/var/lib/flatpak/app/io.github.liberodark.OpenDrive/"
    ],
    "icons_path": [
        "/opt/OpenDrive/resources/",
        "/var/lib/flatpak/app/io.github.liberodark.OpenDrive/current/active/files/OpenDrive/resources/"
    ],
    "binary": "app.asar",
    "script": "electron",
    "icons": {
        "tray": {
            "original": "public/images/logo.png",
            "theme": "odrive-tray"
        },
        "grey": {
            "original": "public/images/logo-grey.png",
            "theme": "odrive-tray-grey"
        },
        "sync": {
            "original": "public/images/logo-sync.png",
            "theme": "odrive-tray-sync"
        }
    }
}
